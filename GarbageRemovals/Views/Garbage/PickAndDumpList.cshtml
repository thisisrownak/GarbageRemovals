@model IEnumerable<GarbageRemovals.Models.PickedAndDump>
    <h3 class="pt-xl-5 pb-xl-3">List of Dump</h3>
<table class="table">
    <thead>
        <tr>
            <th>
               Amount
            </th>
            <th>
               Request
            </th>
            <th>
                Manager
            </th>
            <th>
                Action
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Amount)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Request.AsumedGrabange)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Manager.Email)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
